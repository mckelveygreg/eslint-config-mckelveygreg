{
  "name": "@20i/eslint-config",
  "version": "1.2.1",
  "description": "ESLint and Prettier Config for Twenty Ideas",
  "main": "index.js",
  "author": "Greg McKelvey <mckelveygreg@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/twentyideas/eslint-config-20i#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/twentyideas/eslint-config-20i.git"
  },
  "keywords": [
    "typescript",
    "eslint",
    "lint",
    "config",
    "prettier",
    "style guide",
    "react"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "index.js",
    ".eslintrc.js",
    "react.js",
    "react-native.js",
    "tsconfig.json"
  ],
  "packageManager": "yarn@3.0.2",
  "peerDependencies": {
    "eslint": "^8.3.0",
    "prettier": "^2.5.0",
    "typescript": "^4.5.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^3.11.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.2.1",
    "@types/is-ci": "^3",
    "@types/node": "^16.11.11",
    "eslint": "^8.4.0",
    "husky": ">=7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": ">=12.1.2",
    "markdownlint": "^0.20.4",
    "markdownlint-cli": "^0.23.2",
    "pinst": ">=2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "lint": "yarn eslint --max-warnings=0 --cache --fix \"**/*.{js,ts,jsx,tsx}\"",
    "lint:markdown": "markdownlint --config linters/.markdownlint.json README.md */README.md",
    "postinstall": "is-ci || husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
